const CONTENT_TYPE_CONFIG = {
    quiz: {
        titleSuffix: 'Quiz',
        descriptionPrefix: 'Test your knowledge of',
        icon: _lucidereact.Brain,
        color: 'bg-blue-100 text-blue-800'
