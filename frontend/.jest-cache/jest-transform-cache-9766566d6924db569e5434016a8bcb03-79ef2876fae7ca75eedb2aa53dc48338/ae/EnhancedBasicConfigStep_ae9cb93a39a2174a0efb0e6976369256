                                                id: "time_limit_sec",
                                                type: "number",
                                                ...register('time_limit_sec', {
                                                    valueAsNumber: true
                                                }),
                                                placeholder: "20",
                                                min: 1,
                                                max: 120
                                            })
                                        ]
                                    })
                                ]
                            })
                        ]
                    })
                }),
                selectedPreset && /*#__PURE__*/ (0, _jsxruntime.jsx)(_card.Card, {
                    className: "border-green-200 bg-green-50",
                    children: /*#__PURE__*/ (0, _jsxruntime.jsx)(_card.CardContent, {
                        className: "p-4",
                        children: /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                            className: "flex items-center gap-2",
                            children: [
                                /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                    className: "text-lg",
                                    children: selectedPreset.icon
                                }),
                                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                    children: [
                                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("h4", {
                                            className: "font-medium text-green-900",
                                            children: [
                                                selectedPreset.name,
                                                " Selected"
                                            ]
                                        }),
                                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("p", {
                                            className: "text-sm text-green-700",
                                            children: [
                                                selectedPreset.description,
                                                " â€¢ ",
                                                selectedPreset.estimatedTime
                                            ]
                                        })
                                    ]
                                })
                            ]
                        })
                    })
                })
            ]
        })
    });
};
const _default = EnhancedBasicConfigStep;

