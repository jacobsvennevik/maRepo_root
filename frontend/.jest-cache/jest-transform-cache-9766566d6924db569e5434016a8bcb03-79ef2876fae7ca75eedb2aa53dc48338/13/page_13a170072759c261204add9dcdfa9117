            }),
            viewMode === 'grid' ? /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                className: "grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6",
                children: tests.map((test)=>/*#__PURE__*/ (0, _jsxruntime.jsx)(TestCard, {
                        test: test,
                        onStart: onStartTest
                    }, test.id))
            }) : /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                className: "space-y-4",
                children: tests.map((test)=>/*#__PURE__*/ (0, _jsxruntime.jsx)(TestListItem, {
                        test: test,
                        onStart: onStartTest
                    }, test.id))
            })
        ]
    });
}
// Component: Test Card
function TestCard({ test, onStart }) {
    const getStatusColor = (status)=>{
        switch(status){
            case 'completed':
                return 'bg-green-100 text-green-800';
            case 'upcoming':
                return 'bg-yellow-100 text-yellow-800';
            case 'needs-review':
                return 'bg-red-100 text-red-800';
            default:
                return 'bg-gray-100 text-gray-800';
        }
    };
    const getStatusIcon = (status)=>{
        switch(status){
            case 'completed':
                return /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.CheckCircle, {
                    className: "w-4 h-4"
                });
            case 'upcoming':
                return /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.Clock, {
                    className: "w-4 h-4"
                });
            case 'needs-review':
                return /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.AlertCircle, {
                    className: "w-4 h-4"
                });
            default:
                return /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.Clock, {
                    className: "w-4 h-4"
                });
        }
    };
    return /*#__PURE__*/ (0, _jsxruntime.jsx)(_card.Card, {
        className: "hover:shadow-lg transition-shadow cursor-pointer group",
        children: /*#__PURE__*/ (0, _jsxruntime.jsx)(_card.CardContent, {
            className: "p-6",
            children: /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                className: "space-y-4",
                children: [
                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                        className: "flex items-center justify-between",
                        children: [
                            /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                className: "text-3xl",
                                children: test.icon
                            }),
                            /*#__PURE__*/ (0, _jsxruntime.jsx)(_badge.Badge, {
                                className: getStatusColor(test.status),
                                children: getStatusIcon(test.status)
                            })
                        ]
                    }),
                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                        children: [
                            /*#__PURE__*/ (0, _jsxruntime.jsx)("h3", {
                                className: "font-semibold text-slate-900 mb-1",
                                children: test.title
                            }),
                            /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                                className: "text-sm text-slate-600 mb-2",
                                children: test.subject
                            }),
                            /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                className: "flex items-center gap-2 text-xs text-slate-500",
                                children: [
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.FileText, {
                                        className: "h-3 w-3"
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                        children: [
                                            test.questions,
                                            " questions"
                                        ]
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                        children: "â€¢"
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.Timer, {
                                        className: "h-3 w-3"
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                        children: [
                                            test.timeEstimate,
                                            " min"
                                        ]
                                    })
                                ]
                            })
                        ]
                    }),
                    test.lastScore && /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                        className: "flex items-center gap-2 text-sm",
                        children: [
                            /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.Star, {
                                className: "h-4 w-4 text-yellow-500"
                            }),
                            /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                className: "text-slate-600",
                                children: [
                                    "Last score: ",
                                    test.lastScore,
                                    "%"
                                ]
                            })
                        ]
                    }),
                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                        className: "flex gap-2",
                        children: [
                            /*#__PURE__*/ (0, _jsxruntime.jsxs)(_button.Button, {
                                size: "sm",
                                className: "flex-1",
                                onClick: ()=>onStart(test.id),
                                children: [
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.Play, {
                                        className: "h-3 w-3 mr-1"
                                    }),
                                    test.status === 'completed' ? 'Retake' : 'Start'
                                ]
                            }),
                            /*#__PURE__*/ (0, _jsxruntime.jsx)(_button.Button, {
                                size: "sm",
                                variant: "outline",
                                children: /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.Edit, {
                                    className: "h-3 w-3"
                                })
                            })
                        ]
                    })
                ]
            })
        })
    });
}
