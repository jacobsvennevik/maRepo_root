"use strict";
Object.defineProperty(exports, "__esModule", {
    value: true
});
const _httppath = require("./http-path");
describe('http path helpers', ()=>{
    it('adds trailing slash', ()=>{
        expect((0, _httppath.ensureTrailingSlash)('/users/me')).toBe('/users/me/');
        expect((0, _httppath.ensureTrailingSlash)('/users/me/')).toBe('/users/me/');
    });
    it('rejects /api prefix', ()=>{
        expect(()=>(0, _httppath.assertNoApiPrefix)('/api/users/me/')).toThrow();
        expect((0, _httppath.assertNoApiPrefix)('/users/me/')).toBe('/users/me/');
    });
});

