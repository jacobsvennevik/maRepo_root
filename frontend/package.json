{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "rm -rf .next && next dev",
    "build": "next build",
    "build:notsafe": "next build --no-lint",
    "build:safe": "tsc --noEmit && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest",
    "ci:guard:api-prefix": "(command -v rg >/dev/null 2>&1 && rg -n \"(fetch|axios[A-Za-z]*)\\(\\s*['\\\"]/api/\" src apps packages) || node scripts/ci-guard-api-prefix.js",
    "guard:api": "(command -v rg >/dev/null 2>&1 && rg -n --glob='src/**' \"(fetch\\(|axios(\\.|\\()\\s*['\\\"]/api/|baseURL:\\s*['\\\"]/api)\" ) || node scripts/guard-api-paths.mjs",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watch",
    "test:navigation": "cypress run --spec 'cypress/e2e/navigation.cy.ts'",
    "test:all": "cypress run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headed": "cypress run --headed",
    "test:e2e": "cypress run --spec \"cypress/e2e/school-wizard-*.cy.ts\"",
    "test:e2e:slow": "RUN_SLOW_E2E=true cypress run --spec \"cypress/e2e/school-wizard-*.cy.ts\"",
    "test:e2e:fast": "cypress run --spec \"cypress/e2e/school-wizard-core.cy.ts,cypress/e2e/school-wizard-negative.cy.ts\""
  },
  "dependencies": {
    "@apollo/client": "^4.0.4",
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-avatar": "^1.1.7",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.12",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.4",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.0",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.15.0",
    "graphql": "^16.11.0",
    "lucide-react": "^0.488.0",
    "next": "15.3.0",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-calendar": "^6.0.0",
    "react-day-picker": "^9.7.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.52.1",
    "react-is": "^19.1.0",
    "recharts": "^2.15.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.38"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/eslintrc": "^3",
    "@storybook/addon-a11y": "^8.6.12",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-vite": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@tailwindcss/postcss": "^4.1.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.7.0",
    "cypress": "^14.5.3",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "globby": "^14.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.3",
    "storybook": "^8.6.12",
    "tailwindcss": "^4.1.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5"
  }
}
